{
	"env": {
		"es6": true
	},
	"extends": [
		"eslint:recommended",
		"react-app",
		"airbnb",
		"plugin:jsx-a11y/recommended",
		"prettier",
		"prettier/react"
	],
	"plugins": ["jsx-a11y", "prettier"],
	"rules": {
		"semi": 0,
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"prettier/prettier": ["error", { "semi": true }]
	},
	"parserOptions": {
		"ecmaVersion": 9,
		"sourceType": "module"
	}
}
